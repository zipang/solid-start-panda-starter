{
	"useTabs": true,
	"tabWidth": 4,
	"printWidth": 100,
	"semi": true,
	"endOfLine": "lf",
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"singleQuote": false,
	"trailingComma": "none"
}
